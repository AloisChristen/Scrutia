export class ProjectDTO {
  constructor(
    id,
    title,
    description,
    author,
    date,
    tags,
    nbLikes,
    nbDislikes,
    userVote,
    status
  ) {
    this.id = id
    this.title = title
    this.description = description
    this.author = author
    this.date = date
    this.tags = tags
    this.nbLikes = nbLikes
    this.nbDislikes = nbDislikes
    this.userVote = userVote
    this.status = status
  }
}
